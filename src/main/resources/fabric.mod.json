{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Update Minecraft server resource packs without interrupting gameplay!",
  "authors": [
    "Scubakay"
  ],
  "contact": {
    "issues": "https://github.com/Scubakay/dynamic-resource-pack/issues",
    "sources": "https://github.com/Scubakay/dynamic-resource-pack"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/t8AYBrmqbD"
      }
    }
  },
  "license": "MIT",
  "icon": "assets/dynamic_resource_pack/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "org.scubakay.dynamic_resource_pack.DynamicResourcePack"
    ]
  },
  "mixins": [
    "dynamic_resource_pack.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
