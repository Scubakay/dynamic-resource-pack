{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "Scubakay"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${repository}",
    "issues": "${repository}/issues"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord}"
      }
    }
  },
  "license": "MIT",
  "icon": "assets/dynamic_resource_pack/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "org.scubakay.dynamic_resource_pack.DynamicResourcePack"
    ]
  },
  "mixins": [
    "dynamic_resource_pack.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "minecraft": "${range}",
    "fabric-api": "*"
  }
}
